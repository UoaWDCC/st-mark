  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
